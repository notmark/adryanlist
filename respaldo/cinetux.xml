<title>Estrenos</title>
<link>$doregex[makelist]</link>
<regex>
<name>makelist</name>
<listrepeat><![CDATA[
<title>[makelist.param2]</title>
<link>$doregex[id]</link>
<regex>
<name>id</name>
<expres>file:'(.*)'</expres>
<page>http://streamin.to/embed-$doregex[link]-740x360.html</page>	 
<referer>http://streamin.to</referer>
</regex>
<regex>
<name>link</name>
<expres>http://streamin.to/(.*?)"</expres>
<page>http://www.cinetux.com/pelicula/[makelist.param1]</page>
<referer>http://adryanlist.org</referer>
</regex>
<thumbnail>http:[makelist.param3]</thumbnail>
<fanart>http:[makelist.param3]</fanart>
<info>[makelist.param4]</info>
]]></listrepeat>
<expres>\"poster\" href="/pelicula/(.*?)"[\w\W\s]{0,7}"(.*?)"[\w\W\s]{0,11}"(.*?)"[\w\W\s]{0,5}"(.*?)"</expres>
<page>http://www.cinetux.com/categoria/estrenos/</page>
<referer></referer>
<cookieJar></cookieJar>
</regex>
